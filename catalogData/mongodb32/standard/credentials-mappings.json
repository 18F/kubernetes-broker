{
     "dbname": "$env_MONGO_DBNAME",
     "hostname": "$hostname",
     "password": "$env_MONGO_PASSWORD",
     "port": "$port_27017",
     "ports": {
      "27017/tcp": "$port_27017",
      "28017/tcp": "$port_28017"
     },
     "uri": "mongodb://$env_MONGO_USERNAME:$env_MONGO_PASSWORD@$hostname:$port_27017/$env_MONGO_DBNAME",
     "username": "$env_MONGO_USERNAME"
 }
